<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideToUnlockView">
        <attr format="dimension" name="slideImageViewWidth"/><!-- 滑块宽度 -->
        <attr format="reference" name="slideImageViewResId"/><!-- 滑块资源id -->
        <attr format="reference" name="slideImageViewResIdAfter"/><!-- 滑动到右边时,滑块资源id -->
        <attr format="reference" name="viewBackgroundResId"/><!-- 背景资源id -->
        <attr format="string" name="textHint"/><!-- 文本内容 -->
        <attr format="integer" name="textSize"/><!-- 文本字号 -->
        <attr format="color" name="textColorResId"/><!-- 文本字色 -->
        <attr format="float" name="slideThreshold"/><!-- 滑动阈值,默认是0.5,当右滑距离不满整个控件宽度的0.5,就会回弹至左边 -->
    </declare-styleable>
    <string name="cancel">取消</string>
    <string name="comfirm">确定</string>
    <string name="error_view_loading">视图加载失败</string>
    <string name="loading">加载中</string>
    <string name="retry">重试</string>
    <string name="uploading_flight_point">上传航点</string>
</resources>